@model IEnumerable<Rent.Models.Users>

@{
    ViewBag.Title = "Список пользователей";
}


<h2>Список пользователей</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserSurname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserTelephoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserEmail)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserSurname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserTelephoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserEmail)
        </td>
    </tr>
}

</table>
